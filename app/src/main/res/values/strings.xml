    <!-- Promotions Strings -->
    <string name="promotion_title">How to Promote</string>
    <string name="promotion_message">If you want to see your promotion on the promotions page of our app, just email us the image you want to appear on the page and a URL to go to when the image is pressed. We will then contact you.</string>
    <string name="promotion_email_subject">SLAPP - Promotion Request</string>
    <string name="promotion_email_text">Hi, &#10; &#10; I\'d like to add something to the promotions page on your app...</string>

    <string name="get_promotion_url">http://slapp.dynastysoftware.net:3000/getPromotion</string>
    <string name="promotion_update_needed_url">http://slapp.dynastysoftware.net:3000/promotionUpdateNeeded</string>
    <string name="promotion_image_url">http://slapp.dynastysoftware.net:3000/images/</string>

    <string name="events_title">Add an Event</string>
    <string name="events_message">Email us the following: The title of your events (max 5 words), a description (as long as you want), and a URL (optional), the address of the event, some contact information, and the date and time of your event. We will then contact you.</string>
    <string name="events_email_subject">SLAPP - Event Advertising Request</string>
    <string name="events_email_text">Hi, &#10; &#10; I\'d like advertise my event in your app...</string>

    <string name="get_events_url">http://slapp.dynastysoftware.net:3000/getEvents</string>
    <string name="events_update_needed_url">http://slapp.dynastysoftware.net:3000/eventsUpdateNeeded</string>

    <string name="bug_title">Report a Bug</string>
    <string name="bug_message">Please email us so that we can fix it as soon as possible. Try to be as detailed as possible in the description of the bug (what page you found it on, if it repeats every time, etc).</string>
